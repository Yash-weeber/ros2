source /opt/ros/jazzy/setup.bash
source ~/ros2_ws/install/setup.bash
export ROS_DOMAIN_ID=42




cd ~/ros2_ws
colcon build --packages-select ur_moveit_config --allow-overriding ur_moveit_config
source install/setup.bash



ros2 action send_goal \
  /scaled_joint_trajectory_controller/follow_joint_trajectory \
  control_msgs/action/FollowJointTrajectory \
  '{trajectory: {joint_names: ["shoulder_pan_joint","shoulder_lift_joint","elbow_joint","wrist_1_joint","wrist_2_joint","wrist_3_joint"],
                points: [{positions: [1.117, -1.234, 1.869, -3.694, -0.921, -0.0089],
                          time_from_start: {sec: 5}}]}}'


ros2 launch ur_robot_driver ur_control.launch.py ur_type:=ur5 launch_rviz:=true
ros2 launch ur_moveit_config ur_moveit.launch.py ur_type:=ur5 use_sim_time:=false
ros2 launch ur_moveit_config ur_moveit.launch.py ur_type:=ur5 robot_ip:=192.168.1.102 launch_rviz:=true
ros2 launch ur_robot_driver test_scaled_joint_trajectory_controller.launch.py robot_ip:=192.168.1.102 use_fake_hardware:=false